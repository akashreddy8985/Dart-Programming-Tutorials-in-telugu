void main() {


  var grade = 'F';

  // string and int
  switch(grade){
      case 'A' :
          print('Excellent you got A Grade');
          break;
      case 'B' :
          print('Good Grade');
          break;
      case 'C' :
          print('Not Good You should work hard more.');
          break;
      default:
        print('Invalid grade');
        break;
        
  }

  // if(){
  //   // statement
  // }else if(){
  //   // statement
  // }else if(){

  // }else {

  // }
}
